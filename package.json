{
  "name": "react-groundstation",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.1.0",
    "react-scripts": "0.8.4"
  },
  "dependencies": {
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-rt-chart": "^1.1.4",
    "socket.io": "^1.7.2"
  },
  "scripts": {
    "_pod": "nodemon --watch ./pod pod",
    "_server": "nodemon --watch ./server server",
    "start": "concurrently \"npm run _server\" \"react-scripts start\"",
    "start-all": "concurrently \"npm run _server\" \"npm run _pod\" \"react-scripts start\"",
    "bstart": "npm run build && nodemon server",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}